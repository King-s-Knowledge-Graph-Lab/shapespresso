PREFIX geo: <http://www.opengis.net/ont/geosparql#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX schema: <http://schema.org/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX yago: <http://yago-knowledge.org/resource/>

start = @<MusicComposition>

<MusicComposition> EXTRA rdf:type {
  # schema:Thing property
  rdf:type [ schema:MusicComposition ] ;
  rdfs:comment rdf:langString * ;
  rdfs:label rdf:langString + ;
  schema:alternateName rdf:langString ? ;
  schema:image xsd:anyURI * ;
  schema:mainEntityOfPage xsd:anyURI + ;
  schema:sameAs xsd:anyURI * ;

  # schema.org property
  schema:iswcCode xsd:string ? ;       # From YAGO Schema design document
  schema:lyricist @<Lyricist> * ;      # From YAGO Schema design document
  schema:musicBy @<MusicBy> * ;        # From YAGO Schema design document
  schema:author @<Author> * ;          # From YAGO Schema design document, inherit from @<CreativeWork>
  schema:dateCreated xsd:dateTime ? ;  # From YAGO Schema design document, inherit from @<CreativeWork>
  schema:inLanguage @<Language> * ;    # From YAGO Schema design document, inherit from @<CreativeWork>
  schema:editor @<Person> * ;

  # owl:sameAs
  owl:sameAs [ <http://www.wikidata.org/entity>~ ]
}

<Author> EXTRA rdf:type {
  rdf:type [ schema:Person schema:Organization ]
}

<Language> EXTRA rdf:type {
  rdf:type [ schema:Language ]
}

<Lyricist> EXTRA rdf:type {
  rdf:type [ schema:Person schema:MusicGroup ]
}

<Person> EXTRA rdf:type {
  rdf:type [ schema:Person ]
}

<MusicBy> EXTRA rdf:type {
  rdf:type [ schema:Person schema:MusicGroup ]
}