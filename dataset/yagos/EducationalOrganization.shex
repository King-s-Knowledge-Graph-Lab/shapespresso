PREFIX geo: <http://www.opengis.net/ont/geosparql#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX schema: <http://schema.org/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX yago: <http://yago-knowledge.org/resource/>

start = @<EducationalOrganization>

<EducationalOrganization> EXTRA rdf:type {
  # schema:Thing property
  rdf:type [ schema:EducationalOrganization ] ;
  rdfs:comment rdf:langString * ;
  rdfs:label rdf:langString + ;
  schema:alternateName rdf:langString * ;
  schema:mainEntityOfPage xsd:anyURI + ;
  schema:sameAs xsd:anyURI * ;
  schema:url xsd:anyURI * ;

  # schema.org property
  schema:address xsd:string ? ;
  schema:award @<Award> * ;
  schema:dateCreated xsd:dateTime ? ;  # dateCreated is used on CreativeWork, DataFeedItem types, foundingDate is recommended
  schema:dissolutionDate xsd:dateTime ? ;
  schema:founder @<Person> * ;
  schema:leiCode xsd:string ? ;
  schema:location @<Place> * ;
  schema:locationCreated @<Place> ? ;  # locationCreated is used on CreativeWork types, foundingLocation is recommended
  schema:logo xsd:anyURI ? ;
  schema:slogan xsd:string ? ;
  schema:memberOf @<Organization> * ;
  schema:numberOfEmployees xsd:decimal * ;
  schema:geo geo:wktLiteral ? ;

  # yago property
  yago:studentsCount xsd:decimal * ;  # From YAGO Schema design document
  yago:leader @<Person> * ;
  yago:ownedBy @<OwnedBy> * ;

  # owl:sameAs
  owl:sameAs [ <http://www.wikidata.org/entity>~ ]

}

<Award> EXTRA rdf:type {
  rdf:type [ yago:Award ]
}

<Organization> EXTRA rdf:type {
  rdf:type [ schema:Organization ]
}

<OwnedBy> EXTRA rdf:type {
  rdf:type [ schema:Organization schema:Person ]
}

<Person> EXTRA rdf:type {
  rdf:type [ schema:Person ]
}

<Place> EXTRA rdf:type {
  rdf:type [ schema:Place ]
}